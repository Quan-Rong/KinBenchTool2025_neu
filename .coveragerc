[run]
source = Python_Target/src
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @property
    @.*\.setter

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
